    <script src="<%= ResolveUrl("../../js/inspinia.js") %>"></script>
    <script src="<%= ResolveUrl("../../js/plugins/pace/pace.min.js") %>"></script>

    <!-- jQuery UI -->
    <script src="<%= ResolveUrl("../../js/plugins/jquery-ui/jquery-ui.min.js") %>"></script>

